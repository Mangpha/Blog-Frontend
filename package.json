{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run tailwind:build && next build && next export",
    "start": "next start",
    "lint": "next lint",
    "codegen": "rimraf pages/api/__graphql__ && npx env-cmd -f .env.local apollo client:codegen pages/api/__graphql__ --target=typescript --outputFlat",
    "tailwind:build": "NODE_ENV=production npx tailwindcss -i styles/tailwind.css -o pages/global.css --minify",
    "tailwind:dev": "npx tailwindcss -i styles/tailwind.css -o pages/global.css --minify --watch",
    "sitemap": "sh scripts/generate-sitemap.sh",
    "deploy": "npx serverless@2.72.2"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@headlessui/react": "^1.7.2",
    "animate.css": "^4.1.1",
    "axios": "^1.1.2",
    "dayjs": "^1.11.5",
    "deepmerge": "^4.2.2",
    "globby": "^13.1.2",
    "graphql": "^16.6.0",
    "next": "12.3.1",
    "next-sitemap": "^3.1.28",
    "next-transpile-modules": "^9.0.0",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-animation-on-scroll": "^5.1.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.36.1",
    "react-markdown": "^8.0.3",
    "react-syntax-highlighter": "^15.5.0",
    "react-toggle-dark-mode": "^1.1.0",
    "rehype-raw": "^6.1.1",
    "rehype-stringify": "^9.0.3",
    "remark-breaks": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-normalize-headings": "^3.0.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "18.7.23",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-syntax-highlighter": "^15.5.5",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "autoprefixer": "^10.4.12",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.4"
  }
}
